<%= render 'shared/title', title: "#{@event.title} Organizers", display: 4 %>

<div class='row'>
  <div class='col-12'>
    <div class='card card-block'>
      <h3 class='title'>Add New Organizer</h3>
      <%= render 'admin/organizers/new' %>
    </div>
  </div>
</div>

<div class='row my-4'>
  <div class='col-12'>
    <div class='card card-block'>
      <table class='table'>
        <thead>
          <tr>
            <th>Id</th>
            <th>Organizer Name</th>
            <th>Organizer Email</th>
            <th>Actions</th>
          </tr>
        </thead>
        <tbody id='organizers'>
          <%= render @organizers %>

        </tbody>
      </table>
    </div>
  </div>
</div>