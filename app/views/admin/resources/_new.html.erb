<%= form_for( [:admin, @event, Resource.new ], remote: true, authenticity_token: true) do |f| %>
    <div class='row'>
    <div class='col-md-3'>
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control', id: 'name' %>  
    </div>
    <div class='col-md-6'>
      <%= f.label :attachment %>
      <%= f.file_field :attachment, class: 'form-control-file form-control'%>

    </div>

    <div class='col-md-3' style='padding-top: 1.80rem;'>
      <%= f.submit 'Submit', class: 'btn btn-primary' %>
    </div> 
    </div>
<% end %>
