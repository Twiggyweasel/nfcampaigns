<div class='row my-2'>
  <div class='col-12'>
    <%= link_to(admin_event_path(@event), class: 'btn btn-success') do %><i class="fa fa-arrow-left" aria-hidden="true"></i> Return To Event <% end %>
  </div>
</div>
<%= render 'shared/title', title: "Users attending #{@event.name}", display: 4 %>

<ul>
<% @attendees.each do |attendee| %>
  <li><%= attendee.user.name %></li>
<% end %>
</ul>