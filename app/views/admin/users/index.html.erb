<%= render 'shared/title', title: 'All Users' %>


<div class='container'>
  <div class='row'>
    <div class='col-12'>
      <div class='card card-block'>
        <table class='table mt-3'>
          <thead>
            <tr>
              <th>Name</th>
              <th>Email</th>
              <th>Linked Accounts</th>
              <th>Created On</th>
              <th>Actions</th>
            </tr>
          </thead>
          <tbody>
            <%= render @users %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>