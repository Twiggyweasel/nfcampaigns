<!DOCTYPE html>
<html>
  <head>
    <title>Strong Strides for NF</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
        <link href="https://fonts.googleapis.com/css?family=Changa+One|Boogaloo|Just+Another+Hand" rel="stylesheet">
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= favicon_link_tag %>
  </head>

  <body>
    <%= render 'shared/nav' %>
    <%= render 'shared/messages' %>
    <div class='container-fluid px-0'>
      <%= yield %>
    </div>
  </body>
</html>
